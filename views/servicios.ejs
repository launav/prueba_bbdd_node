<%- include('templates/header',{ title: 'Nuestros servicios' , descripcion: 'Esta es la descripcion de servicios' })%>

    <main class="my-5 container">
        <h1>
            <!-- ejsout -->
            <%= titulo %>
        </h1>
        <p>
            <%= descripcion %>
        </p>
        <section class="mt-5">
            <% data.forEach(element=> { %>
                <article class="card mb-3">
                    <div class="card-body">
                        <h2 class="card-title"><%= element.nombre %> </h2>
                        <p class="card-text"><%= element.comentario %></p>
                    </div>
                </article>

                <% }) %>
        </section>
    </main>

    <%- include('templates/footer')%>